#include <iostream>

using namespace std;

int main()
{
	int q = 1;
	int w = 10;
	int e = 100;
	int r = 1000;
	int t = 10000;
	int y = 100000;

	cout << q << "\n" << w << "\n" << e << "\n" << r << "\n" << t << "\n" << y << "\n";
	cout << "\n";

	double a = 1;
	double s = 10;
	double d = 100;
	double f = 1000;
	double g = 10000;
	double h = 100000;

	cout << a << "\n" << s << "\n" << d << "\n" << f << "\n" << g << "\n" << h << "\n";

	return 0;
}

// There is no difference between integers and floating point without a specified decimal? It might compile differently on Cygwin
