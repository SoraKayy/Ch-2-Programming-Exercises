#include <iostream>
#include <string>

using namespace std;

int main()
{
	string comb = "+--+--+--+";
	string lines = "|  |  |  |";

	cout << comb << "\n";
	cout << lines << "\n";
	cout << comb << "\n";
	cout << lines << "\n";
	cout << comb << "\n";
	cout << lines << "\n";
	cout << comb << "\n";

	return 0;
}

