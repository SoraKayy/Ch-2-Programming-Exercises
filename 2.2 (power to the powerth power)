#include <iostream>
#include <cmath>

using namespace std;

int main()
{
	cout << "1 \n"; //1 to the 'n'th power
	cout << "------------ \n";
	cout << pow(1, 2) << "\n";
	cout << pow(1, 3) << "\n";
	cout << pow(1, 4) << "\n";
	cout << "\n";

	cout << "2 \n"; //2 to the 'n'th power
	cout << "------------ \n";
	cout << pow(2, 2) << "\n";
	cout << pow(2, 3) << "\n";
	cout << pow(2, 4) << "\n";
	cout << "\n";

	cout << "3 \n"; //3 to the 'n'th power
	cout << "------------ \n";
	cout << pow(3, 2) << "\n";
	cout << pow(3, 3) << "\n";
	cout << pow(3, 4) << "\n";
	cout << "\n";

	cout << "4 \n"; //4 to the 'n'th power
	cout << "------------ \n";
	cout << pow(4, 2) << "\n";
	cout << pow(4, 3) << "\n";
	cout << pow(4, 4) << "\n";
	cout << "\n";

	cout << "5 \n"; //5 to the 'n'th power
	cout << "------------ \n";
	cout << pow(5, 2) << "\n";
	cout << pow(5, 3) << "\n";
	cout << pow(5, 4) << "\n";
	cout << "\n";

	return 0;
}

